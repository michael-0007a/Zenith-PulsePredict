{
  "version": 2,
  "builds": [
    {
      "src": "api/predict.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "500mb",
        "runtime": "python3.9",
        "buildCommand": "bash vercel_build.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/predict",
      "dest": "api/predict.py"
    }
  ]
}
